page {
 width: 100%;
 height: 100%; 
}
.main {
  height: 100%;
}
.app-name {
  color: #515151;
  padding-top: 24%;
  text-align: center;
  font-family: "思源黑体";
  font-weight: bold;
  font-size: 34rpx;
}
.section {
  height: 8%;
  margin: 14% 14%;
  border-bottom:1px solid #C9C9C9;
}
.section_password{
  height: 8%;
  margin: 14% 14%;
  margin-top: -4%;
  border-bottom:1px solid #C9C9C9;
}
.account {
  width: 8%;
  height: 48%;
  float: left;
  margin-left: 5%; 
  margin-top: 8%;
}
.password {
  width: 9%;
  height: 54%;
  float: left;
  margin-left: 5%; 
  margin-top: 7.2%;
}
.input{
  padding-top: 8.8%;
  padding-left: 18%;
  font-family: "思源黑体";
  font-weight: Regular;
  font-size: 26rpx;
  color: #A1A1A1;
  text-align: left;
}
.commit::after{
  border: none;
}
.commit{
  height: 96rpx;
  margin: 14% 14%;
  margin-top: 20%;
  border-radius: 100px;
  background: #1296DB;
  color: white;
  font-family: "思源黑体";
  font-weight: Medium;
  font-size: 32rpx;
  padding-top: 8rpx;
}